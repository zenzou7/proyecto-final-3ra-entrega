<body>

    <h1>REGISTRO</h1>
<form method="POST" action="/api/productos/signup">
    <input placeholder="username" name="username" required/>
    <input placeholder="password" name="password" required/>
    <input placeholder="email" name="email" required/>
    <input placeholder="number" name="number" required/>
    <input placeholder="avatar" name="avatar" required/>
    <input type="submit" value="registrarse" />
</form>
<a href="/api/productos/login">Ir a login</a>
    
</body>
